{
  "name": "legitify-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "main": "./dist/main.js",
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node ./src/database/seeders/country.seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "gen:schema": "cd ./prisma/models && cat *.prisma > ../schema.prisma",
    "db:restart": "docker-compose down && docker-compose up -d",
    "migrate": "yarn run gen:schema && npx prisma generate --schema prisma/schema.prisma",
    "db:seed": "npx prisma db seed",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env TZ=UTC NODE_ENV=${NODE_ENV:=dev} tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "start:dev": "yarn start --watch",
    "start:stg": "cross-env TZ=UTC NODE_ENV=${NODE_ENV:=stg} node .",
    "start:prod": "cross-env TZ=UTC NODE_ENV=${NODE_ENV:=prod} node .",
    "start:demo": "cross-env TZ=UTC NODE_ENV=${NODE_ENV:=demo} node .",
    "start:debug": "yarn start --debug --watch",
    "kill": "sudo pkill -9 node",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test jest --config jest.config.json --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy:assets": "cp -r ./src/assets ./dist/assets",
    "postinstall": "yarn migrate"
  },
  "dependencies": {
    "@nestjs/axios": "2.0.0",
    "@nestjs/cache-manager": "2.1.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/event-emitter": "1.4.1",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/platform-ws": "10.0.5",
    "@nestjs/schedule": "2.2.1",
    "@nestjs/schematics": "9.1.0",
    "@prisma/client": "^4.12.0",
    "amazon-cognito-identity-js": "5.2.10",
    "bcrypt": "5.1.0",
    "cache-manager": "5.2.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cross-env": "7.0.3",
    "dayjs": "1.11.7",
    "dotenv-cli": "^7.2.1",
    "file-type": "18.4.0",
    "form-data": "4.0.0",
    "generate-password": "1.7.1",
    "helmet": "^6.1.5",
    "jwks-rsa": "3.0.1",
    "jwt-decode": "3.1.2",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "prisma": "4.12.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.0",
    "rxjs": "7.8.0",
    "uuid": "9.0.0",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.4.0",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@nestjs/testing": "9.4.0",
    "@types/bcrypt": "5.0.0",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/multer": "1.4.7",
    "@types/node": "18.15.11",
    "@types/passport": "1.0.15",
    "@types/passport-http": "0.3.11",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.1",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "passport-headerapikey": "1.2.2",
    "prettier": "2.8.7",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  }
}
